/* miniprogram/pages/index/index.wxss */
.canvas{
  margin-top: 80rpx;
  width: 200px;
  height: 200px;
}
/* 自制表格 */
.table{
  border-collapse:collapse; 
  border:1px solid #080808;
  border-right:0;
  border-bottom: 0;
  width: 80%;
  margin-left: 1%;
}
.tr{
  width:100%;
  display: flex;
  justify-content: space-between;
}
.th,.td{
  padding: 10px;
  border-bottom: 1px solid #080808;
  border-right: 1px solid #080808;
  text-align: center;
  width: 100%;
}

.th{
  font-size: 20rpx;
  color: #330e0e;
}
.td{
  font-size: 20rpx;
 
}

.birthday{
  padding-left:100rpx;
  padding-top: 20rpx;
}
.center{
  display:flex;
  justify-content:center;
  align-items:center;
  }
.btn{

  background-color:#8bf7ee;
  height:80rpx;
  width:600rpx;
  
}
.cake{
  position: absolute;
  
}
 
@keyframes dropdown {
  from{margin-top: -50rpx;}
  to{margin-top: 1800rpx;}
}
.clock2{
  border:1px solid rgb(167, 163, 163);
  padding:5px;
  
  font-size: 13px;
}
.td2{
  border:1px solid gray;
  font-size: 20rpx;
  display: inline;
}
