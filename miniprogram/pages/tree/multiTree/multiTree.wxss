/* 导入全局样式 */
@import "../../../app.wxss";
page{
  height: 100%;
}
/* 顶部的搜索栏和我的树屋 */
.head{
  text-align: center;
  display: flex;
  
  background-color:#fbfcdb;
 
}

.allMyHouse{
  margin:0 20rpx;
  /* text-align: center; */
  padding-top: 20rpx;
  width: 600rpx;
  height: 80rpx;
  border-radius: 5%;
  
}
.head image{
  padding-top: 15rpx;
  width: 70rpx;
  height: 70rpx;
}
/* 底部的创建树屋 */
.add{
  position: fixed;
  width: 100rpx;
  height: 100rpx;
  opacity: 0.5;
  right: 30rpx;
  bottom: 30rpx;
}
/* 加载情况的文本 */
.text1{
  color: grey;
  font-size: 40rpx;
  padding:20rpx 0rpx;
  text-align: center;
}
