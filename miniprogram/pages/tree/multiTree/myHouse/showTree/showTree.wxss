/* 样式：流动的云 
  从左到右，10S完成*/

.flowingCloud {
  position: absolute;
  z-index: 3;
  width: 50%;
  height: 50%;
  top: 180rpx;
  right: 0;
  bottom: 0;
  animation: cloud 10s linear infinite;
}

@keyframes cloud {
  from {
    transform: translate3d(-700rpx, 0, 0);
  }

  to {
    transform: translate3d(300rpx, 0, 0);
  }
}

/* 静止的云 */
.staticCloud {
  position: relative;
  width: 30%;
  height: 30%;
  left: 65%;
  top: 80rpx;
}





.tree {
  bottom: 100rpx;
  left: 0;
  right: 0;
  padding: 0px;
  margin: 0px;
  text-align: center;
  position: absolute;
  z-index: 3;
}

/*  */
.treeOut {
  position: absolute;
  top: -70rpx;
  left: 440rpx;
  /* height: 100rpx;
      width: 300rpx; */
  border: 1px solid;
  border-radius: 8%;
  border-color: rgb(104, 243, 106);
  color: rgb(104, 243, 106);
  /* 将对话框设为不可见，开始移动的时候再设为可见 */
  visibility: hidden;

}

/* 对话框 */
.dialogue {
  visibility: visible;
  animation: dialogue 5s;
}

@keyframes dialogue {

  from {
    top: -10rpx;
    visibility: visible;
  }

  to {
    top: -40rpx;
    visibility: visible;
  }

}

