<!--miniprogram/pages/tree/multiTree/myHouse/introTohouse/setgoal/setgoal.wxml-->
<view class="my-goal">
	<view class="my-goal-content">
		<image src="https://i.loli.net/2020/05/07/YK5MceV41qDxjAr.jpg" mode="widthFix" style="width:100%;padding-bottom:10rpx"></image>
		<text>我的目标</text>
		<input bindinput="bindGoal" placeholder="给自己定个小目标吧" placeholder-class="placeholder-inpunt"></input>
		<text>预计完成时间</text>
		<input bindinput="bindTime" placeholder="准时完成它哦" placeholder-class="placeholder-inpunt"></input>
		<button bindtap="bindSubmit" plain hover-class>提交</button>
	</view>
  <mp-toptips msg="{{tips}}" type="{{type}}" show="{{tips}}"></mp-toptips>
</view>